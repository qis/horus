{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 22
  },
  "configurePresets": [
    {
      "name": "default",
      "displayName": "Default",
      "binaryDir": "${sourceDir}/build",
      "installDir": "${sourceDir}/release",
      "generator": "Ninja",
      "architecture": {
        "value": "x64",
        "strategy": "external"
      },
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_MSVC_RUNTIME_LIBRARY": "MultiThreadedDLL",
        "CMAKE_RC_FLAGS": "/nologo -DWIN32",
        "CONAN_CMAKE_SILENT_OUTPUT": true,
        "OPENCV_EXTRA_MODULES_PATH": "${sourceDir}/contrib/modules",
        "BUILD_SHARED_LIBS": true,
        "BUILD_WITH_STATIC_CRT": false,
        "BUILD_opencv_dnn": false,
        "BUILD_opencv_julia": false,
        "BUILD_opencv_matlab": false,
        "BUILD_opencv_ovis": false,
        "BUILD_opencv_sfm": false,
        "BUILD_opencv_ts": false,
        "BUILD_opencv_viz": false,
        "BUILD_opencv_highgui": true,
        "BUILD_opencv_photo": false,
        "BUILD_opencv_video": false,
        "BUILD_opencv_videoio": false,
        "BUILD_opencv_python2": false,
        "BUILD_opencv_python3": false,
        "BUILD_opencv_java": false,
        "BUILD_DOCS": false,
        "BUILD_EXAMPLES": false,
        "BUILD_TESTS": false,
        "BUILD_JAVA": false,
        "WITH_VTK": false,
        "WITH_CUDA": true,
        "WITH_CUDNN": false,
        "WITH_NVCUVID": false,
        "WITH_NVCUVENC": false,
        "WITH_FFMPEG": false,
        "WITH_GSTREAMER": false,
        "WITH_GTK": false,
        "WITH_GTK_2_X": false,
        "WITH_IPP": true,
        "WITH_HALIDE": false,
        "WITH_VULKAN": false,
        "WITH_OPENVINO": false,
        "WITH_WEBNN": false,
        "WITH_JASPER": false,
        "WITH_OPENJPEG": false,
        "WITH_JPEG": false,
        "WITH_WEBP": false,
        "WITH_OPENEXR": false,
        "WITH_OPENGL": false,
        "WITH_OPENVX": false,
        "WITH_OPENNI": false,
        "WITH_OPENNI2": false,
        "WITH_PNG": true,
        "WITH_SPNG": false,
        "WITH_GDCM": false,
        "WITH_PVAPI": false,
        "WITH_ARAVIS": false,
        "WITH_QT": false,
        "WITH_WIN32UI": true,
        "WITH_TBB": false,
        "WITH_HPX": false,
        "WITH_TIFF": false,
        "WITH_DSHOW": false,
        "WITH_MSMF": false,
        "WITH_XIMEA": false,
        "WITH_UEYE": false,
        "WITH_XINE": false,
        "WITH_CLP": false,
        "WITH_OPENCL": false,
        "WITH_OPENCLAMDFFT": false,
        "WITH_OPENCLAMDBLAS": false,
        "WITH_DIRECTX": false,
        "WITH_LIBREALSENSE": false,
        "WITH_VA": false,
        "WITH_MFX": false,
        "WITH_GDAL": false,
        "WITH_GPHOTO2": false,
        "WITH_PROTOBUF": false,
        "WITH_IMGCODEC_HDR": false,
        "WITH_IMGCODEC_SUNRASTER": false,
        "WITH_IMGCODEC_PXM": false,
        "WITH_IMGCODEC_PFM": false,
        "WITH_QUIRC": false,
        "WITH_ONNX": false,
        "WITH_TIMVX": false,
        "WITH_OBSENSOR": false,
        "WITH_CANN": false,
        "WITH_FREETYPE": true,
        "INSTALL_CREATE_DISTRIB": true,
        "ENABLE_FAST_MATH": true,
        "ENABLE_LTO": true,
        "CV_TRACE": false
      },
      "environment": {
        "PATH": "C:/OBS/obs-plugins/64bit;$penv{PATH}"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "release",
      "displayName": "Release",
      "configuration": "Release",
      "configurePreset": "default"
    }
  ]
}
